    "lodash": "^4.17.21",
    "mongoose": "^6.2.7",
    "nanoid": "^4.0.0",
    "pino": "^8.4.1",
    "pino-pretty": "^9.0.0",
    "pm2": "^5.2.0",
    "ts-node-dev": "^2.0.0",
    "yup": "^0.32.11",
    "agentkeepalive": "^4.2.1",
    "throttle": "1.x"
